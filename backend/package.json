{
  "name": "backend",
  "module": "src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --outdir ./dist --target node",
    "start": "bun run src/server.ts",
    "test": "bun test",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down",
    "migrate:create": "migrate-mongo create",
    "seed": "bun run src/database/seed.ts",
    "seed:permissions": "bun run src/database/seed.ts permissions",
    "seed:users": "bun run src/database/seed.ts users",
    "seed:products": "bun run src/database/seed.ts products",
    "seed:all": "bun run src/database/seed.ts all",
    "seed:clear": "bun run src/database/seed.ts clear",
    "seed:reset": "bun run src/database/seed.ts reset"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/multer": "^2.0.0",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@types/nodemailer": "^7.0.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "csv-parser": "^3.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
    "express-routes-versioning": "^1.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "migrate-mongo": "^11.0.0",
    "mongoose": "^8.7.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  }
